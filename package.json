{
  "name": "spacesaverapp",
  "version": "1.0.0",
  "description": "A comprehensive disk space management application for macOS",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "macos": "react-native run-macos",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm test",
    "clean": "rm -rf node_modules && npm install",
    "setup:macos": "bash scripts/setup-macos.sh",
    "pod:install": "cd macos && pod install --repo-update || echo 'CocoaPods required - install with: brew install cocoapods'",
    "pod:clean": "cd macos && rm -rf Pods Podfile.lock && pod install --repo-update",
    "build:macos": "react-native build-macos --mode Release"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-native": "0.77.0",
    "react-native-macos": "0.77.0",
    "zustand": "^4.5.0",
    "date-fns": "^3.3.1",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/runtime": "^7.24.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.77.0",
    "@react-native/metro-config": "0.77.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/react": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "0.77.0",
    "react-test-renderer": "18.3.1",
    "typescript": "5.4.2"
  },
  "engines": {
    "node": ">=18"
  },
  "private": true
}
